@model DevFolio.Models.TblProfile
@{
    ViewBag.Title = "UpdateProfile";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Profili Güncelle</h2>
<form method="post">
    <label>Adı Soyadı</label>
    <input type="text" class="form-control" name="NameSurname" value="@Model.NameSurname" />
    <br />
    <label>Görev</label>
    <input type="text" class="form-control" name="Title" value="@Model.Title" />
    <br />
    <label>E-Posta</label>
    <input type="text" class="form-control" name="Email" value="@Model.EMail"/>
    <br />
    <label>Telefon</label>
    <input type="text" class="form-control" name="Phone" value="@Model.Phone" />
    <br />
    <button class="btn btn-info">Profili Güncelle</button>

</form>


